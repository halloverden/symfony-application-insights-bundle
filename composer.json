{
    "name": "halloverden/symfony-application-insights-bundle",
    "description": "Enables Azure Application Insights monitoring",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Giannetti",
            "email": "marco@braaains.no"
        }
    ],
    "keywords": [
        "azure",
        "application insights",
        "logging",
        "exception",
        "symfony"
    ],
    "support": {
        "issues": "https://github.com/halloverden/symfony-application-insights-bundle/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php":"^7.2",
        "symfony/http-kernel": "^4.4.7|^5.0.7",
        "symfony/dependency-injection": "^4.4.7|^5.0.7",
        "symfony/config": "^4.4.7|^5.0.7",
        "symfony/routing": "^4.4.7|^5.0.7",
        "halloverden/application-insights-sdk": "@dev",
        "ext-json": "*",
        "symfony/cache": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "HalloVerden\\ApplicationInsightsBundle\\": ""
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../halloverden/application-insights-sdk",
            "options": {
                "symlink": true
            }
        }
    ]
}
